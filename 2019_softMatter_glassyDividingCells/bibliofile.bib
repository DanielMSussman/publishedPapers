@article{Streichan2014,
abstract = {Control of cell proliferation is a fundamental aspect of tissue for- mation in development and regeneration. Cells experience various spatial and mechanical constraints depending on their environ- mental context in the body, but we do not fully understand if and how such constraints influence cell cycle progression and thereby proliferation patterns in tissues. Here, we study the impact of mechanical manipulations on the cell cycle of individual cells within a mammalian model epithelium. By monitoring the response to experimentally applied forces, we find a checkpoint at the G1–S boundary that, in response to spatial constraints, controls cell cycle progression. This checkpoint prevents cells from entering S phase if the available space remains belowa characteristic threshold because of crowding. Stretching the tissue results in fast cell cycle reactiva- tion, whereas compression rapidly leads to cell cycle arrest. Our kinetic analysis of this response shows that cells have no memory of past constraints and allows us to formulate a biophysical model that predicts tissue growth in response to changes in spatial con- straints in the environment. This characteristic biomechanical cell cycle response likely serves as a fundamental control mechanism to maintain tissue integrity and to ensure control of tissue growth during development and regeneration. cell},
author = {Streichan, S. J. and Hoerner, C. R. and Schneidt, T. and Holzer, D. and Hufnagel, L.},
doi = {10.1073/pnas.1323016111},
file = {:Users/chigantic/Library/Application Support/Mendeley Desktop/Downloaded/Streichan et al. - 2014 - Spatial constraints control cell proliferation in tissues.pdf:pdf},
isbn = {1091-6490; 0027-8424},
issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences},
keywords = {Animals,Biomechanical Phenomena,Biophysics,Cell Cycle Checkpoints,Cell Cycle Checkpoints: physiology,Cell Proliferation,Contact Inhibition,Contact Inhibition: physiology,Dogs,Kinetics,Madin Darby Canine Kidney Cells,Models, Biological},
number = {15},
pages = {5586--5591},
pmid = {24706777},
title = {{Spatial constraints control cell proliferation in tissues}},
url = {http://www.pnas.org/cgi/doi/10.1073/pnas.1323016111},
volume = {111},
year = {2014}
}

