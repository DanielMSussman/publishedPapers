issn = {1742-5662},
journal = {Journal of the Royal Society, Interface / the Royal Society},
keywords = {Cell Communication,Cell Movement,Cell Movement: physiology,Cell Polarity,Cell Size,Computer Simulation,Epithelial Cells,Neoplasm Invasiveness,Signal Transduction},
month = dec,
number = {77},
pages = {3268--78},
pmid = {22832363},
title = {{Collective cell migration: leadership, invasion and segregation.}},
url = {http://rsif.royalsocietypublishing.org/content/early/2012/07/23/rsif.2012.0448.short},
volume = {9},
year = {2012}
